{
  "name": "tscute",
  "version": "0.2.0",
  "description": "A CLI to execute TypeScript",
  "type": "module",
  "bin": {
    "tscute": "build/tscute.js"
  },
  "exports": {
    "./*.js": "./build/*",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "start": "node build/tscute.js"
  },
  "dependencies": {
    "@swc/core": "^1.13.5",
    "oxc-resolver": "^11.6.2",
    "tsconfck": "^3.1.6",
    "tsconfig-migrate": "^0.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@startracex/dev-config": "^0.5.0",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "typescript",
    "execute"
  ],
  "author": "startracex",
  "license": "MIT",
  "files": [
    "**/*.{d.ts,js}+(|.map)",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/tscute.git"
  },
  "packageManager": "pnpm@10.15.0"
}
