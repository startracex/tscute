{
  "name": "tscute",
  "version": "0.5.0",
  "description": "A way to execute/import TypeScript",
  "type": "module",
  "bin": {
    "tscute": "build/bin/tscute.js"
  },
  "main": "build/loader-register.js",
  "exports": {
    ".": "./build/loader-register.js",
    "./*.js": "./build/*.js",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc",
    "fmt": "dprint fmt",
    "start": "node build/bin/tscute.js"
  },
  "dependencies": {
    "@swc/core": "^1.13.5",
    "oxc-resolver": "^11.8.4",
    "tsconfck": "^3.1.6",
    "tsconfig-migrate": "^0.1.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@startracex/dev-config": "^0.5.0",
    "@types/node": "^24.6.1",
    "dprint": "^0.50.2",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "typescript",
    "execute"
  ],
  "author": "startracex",
  "license": "MIT",
  "files": [
    "**/*.{d.ts,js}+(|.map)",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/tscute.git"
  },
  "packageManager": "pnpm@10.18.0"
}
